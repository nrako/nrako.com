{
  "name": "nrako.com",
  "version": "0.0.1",
  "description": "nrako.com website",
  "main": "index.js",
  "repository": "git@github.com:nrako/nrako.com.git",
  "author": "Nicholas Rakoto <nico@nrako.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next",
    "export": "next export",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --list-different \"**/*.{js,json,md}\"",
    "start": "next start",
    "test": "jest",
    "test:ci": "docker-compose -p tests run -p 3000 --rm web npm run build && npm run test",
    "test:ciu": "docker-compose -p tests run -p 3000 --rm web npm run test:_ciu",
    "test:_ciu": "next build && jest -u"
  },
  "dependencies": {
    "next": "^6.0.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "chalk": "^2.3.2",
    "eslint": "^4.19.1",
    "jest": "^22.4.3",
    "jest-image-snapshot": "^2.4.0",
    "prettier": "^1.12.0",
    "puppeteer": "^1.2.0",
    "rimraf": "^2.6.2"
  },
  "now": {
    "type": "npm",
    "env": {
      "NODE_ENV": "production"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}

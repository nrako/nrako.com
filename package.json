{
  "name": "nrako.com",
  "version": "0.0.1",
  "description": "nrako.com website",
  "main": "index.js",
  "repository": "git@github.com:nrako/nrako.com.git",
  "author": "Nicholas Rakoto <nico@nrako.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next",
    "export": "next export",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --list-different \"**/*.{js,json,md}\"",
    "start": "next start",
    "test": "jest",
    "test:ci": "docker-compose exec test npm run test",
    "test:ciu": "docker-compose exec test npm run test:_ciu",
    "test:_ciu": "next build && jest -u",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@zeit/next-mdx": "^1.2.0",
    "next": "^10.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "remark-highlight.js": "^6.0.0",
    "snyk": "^1.425.4"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^1.5.8",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.3.0",
    "jest-image-snapshot": "^3.0.1",
    "jest-puppeteer": "^4.4.0",
    "prettier": "^2.0.4",
    "puppeteer": "^3.0.0",
    "rimraf": "^3.0.2"
  },
  "now": {
    "type": "npm",
    "env": {
      "NODE_ENV": "production"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "package.json",
        "options": {
          "printWidth": 120
        }
      }
    ]
  },
  "snyk": true
}
